<%= render("layouts/errors", :object => @stop) %>

<%= form_for(@stop) do |f| %>

  <%= f.collection_select :station_id, Station.all, :id, :name, prompt: "Select Station" %>
  <%= f.collection_select :line_id, Line.all, :id, :name, prompt: "Select Line" %>
  <%= f.submit(:class => "btn btn-success") %>
<% end %>
